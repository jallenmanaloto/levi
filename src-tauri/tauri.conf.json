{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "levi",
  "version": "0.1.0",
  "identifier": "com.levi.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "levi",
        "width": 800,
        "height": 900,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/128x128@2x.png"
    ]
  }
}
